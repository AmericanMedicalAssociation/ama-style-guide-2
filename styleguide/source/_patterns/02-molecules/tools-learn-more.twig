<div class="ama__tools__learn-more">
  {% set heading = learnMore.heading %}
  {% block learMoreHeading %}
    {% include "@atoms/heading.twig" %}
  {% endblock %}
  <ul>
    {% block learMoreLinks %}
      {% for link in learnMore.links %}
        {% set link = link|merge({ "class": "ama__link--black" }) %}
        <li>
          {% include '@atoms/link.twig' %}
        </li>
      {% endfor %}
    {% endblock %}
  </ul>
</div>
