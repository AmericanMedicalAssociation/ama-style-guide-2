<div class="dr-finder-edit-header-no-image">
    <div class="dr-finder-edit-header-no-image__image-default">{{ drFinderDoctorEditHeaderNoImage.doctorImageDefault }}</div>
    <div class="dr-finder-edit-header-no-image__doctor-info">
        <div class="dr-finder-edit-header-no-image__doctor-info_name ama__h3">
          <b>{{ drFinderDoctorEditHeaderNoImage.doctorInfo.name }}</b>
        </div>
        <div class="dr-finder-edit-header-no-image__doctor-info_street ama__h5">{{ drFinderDoctorEditHeaderNoImage.doctorInfo.address.street }}</div>
        <div class="dr-finder-edit-header-no-image__doctor-info_city ama__h5">{{ drFinderDoctorEditHeaderNoImage.doctorInfo.address.city }}</div>
        <div class="dr-finder-edit-header-no-image__doctor-info_number ama__h5">{{ drFinderDoctorEditHeaderNoImage.doctorInfo.phoneNumber }}</div>
        <div class="dr-finder-edit-header-no-image__doctor-info_specialty"><b>{{ drFinderDoctorEditHeaderNoImage.doctorInfo.specialty }}</b></div>
    </div>
</div>
