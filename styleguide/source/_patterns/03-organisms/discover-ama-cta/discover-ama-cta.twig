<div class="ama__discover-ama-cta">
  <div class="ama__discover-ama-cta__header">
    <h1 class="ama__discover-ama-cta__title">{{ title }}</h1>
  </div>
  <div class="ama__discover-ama-cta__links">
    {% for item in links %}
      <a class="ama__discover-ama-cta__links__item" href="{{ item.url }}" target="_blank">
        <span href="{{ item.url }}" class="ama__discover-ama-cta__link">{{ item.title }}</span>
		<i class="ama__discover-ama-cta__link__arrow"></i>
      </a>
    {% endfor %}
  </div>
</div>
