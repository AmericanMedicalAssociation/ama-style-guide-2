<div class="homepage-social-module no-feature">
  <div class="homepage-social-module__container">
    <div class="profile">
      <div class="profile--image">
        {% set image = profile_1.image %}
        {% include "@atoms/image/image.twig" %}
      </div>
      <div class="profile--content">
        {% include "@atoms/heading/heading.twig"  with { heading : profile_1.heading } %}
        {% include '@atoms/paragraph.twig' with { paragraph : profile_1.paragraph_location } %}
        {% set link = profile_1.link %}
        {% include "@atoms/link/link.twig" with { link : profile_1.link } %}
      </div>
    </div>
    <div class="profile">
      <div class="profile--image">
        {% set image = profile_2.image %}
        {% include "@atoms/image/image.twig" %}
      </div>
      <div class="profile--content">
        {% include "@atoms/heading/heading.twig"  with { heading : profile_2.heading } %}
        {% include '@atoms/paragraph.twig' with { paragraph : profile_2.paragraph_location } %}
        {% set link = profile_2.link %}
        {% include "@atoms/link/link.twig" with { link : profile_2.link } %}
      </div>
    </div>
  </div>
  <div class="hs_message">
    <p>If you or someone you know should be featured in our Ambassador Spotlight, please email <a href="href="mailto:ambassadors@ama-assn.org"">ambassadors@ama-assn.org</a>.</p>
  </div>
</div>
